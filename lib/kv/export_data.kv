#:kivy 1.11.1
<ExportData>:
    padding: dp(10)
    spacing: dp(2.5)
    MDBoxLayout:
        id: tree_frame
    MDBoxLayout:
        width: dp(60)
        size_hint_x: None
        orientation: 'vertical'
        md_bg_color: app.theme_cls.primary_dark
        MDTooltipIconButton:
            height: dp(50)
            icon: 'refresh'
            pos_hint: {'center_x': .5}
            tooltip_text: 'Atualizar √Årvore'
            on_release:
                root.switch_button(True)
                root.data_tree.regen_tree()
        MDSeparator:
            height: dp(2.5)
            pos_hint: {'center_x': .5}
            color: app.theme_cls.bg_light
        MDTooltipIconButton:
            height: dp(50)
            icon: root.btn_icon[0]
            pos_hint: {'center_x':.5}
            tooltip_text: root.btn_text[0]
            on_release:
                root.switch_button()
                root.data_tree.toggle_all_nodes()
        MDSeparator:
            height: dp(2.5)
            pos_hint: {'center_x': .5}
            color: app.theme_cls.bg_light
        MDTooltipIconButton:
            height: dp(50)
            pos_hint: {'center_x': .5}
            icon: 'file-export-outline'
            tooltip_text: 'Exportar .CSV'
            on_release: root.confirm_export_dialog()
        MDSeparator:
            height: dp(2.5)
            pos_hint: {'center_x': .5}
            color: app.theme_cls.bg_light
        MDLabel:
            size_hint_y: 1